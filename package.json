{
    "name": "nextra-theme-dsfr-docs",
    "version": "0.0.0",
    "description": "DSFR theme for Nextra",
    "repository": {
        "type": "git",
        "url": "git://github.com/codegouvfr/nextra-theme-dsfr-docs.git"
    },
    "scripts": {
        "build": "tsc",
        "test": "echo TODO",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "link-in-app": "ts-node --skipProject scripts/link-in-app.ts"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/codegouvfr",
    "license": "MIT",
    "files": [
        "src/",
        "dist/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/garronej/update-ts-ci",
    "peerDependencies": {
        "next": "^13.4.7",
        "nextra": "^2.8.0",
        "react": ">=16.13.1",
        "react-dom": ">=16.13.1",
        "@codegouvfr/react-dsfr": "^0.65.0",
        "@mui/material": "^5.13.6",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@emotion/server": "^11.11.0"
    },
    "dependencies": {
        "tss-react": "^4.8.6"
    },
    "devDependencies": {
        "@types/node": "^20.2.1",
        "typescript": "^5.0.4",
        "@typescript-eslint/parser": "^5.59.6",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "react": "^18.2.0",
        "@types/react": "^18.2.14",
        "@codegouvfr/react-dsfr": "^0.65.0",
        "@mui/material": "^5.13.6",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "next": "^13.4.7",
        "nextra": "^2.8.0",
        "@emotion/server": "^11.11.0"
    }
}
